{
  "version": 2,
  "name": "everpeak-nature-camp",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
